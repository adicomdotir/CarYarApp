<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_contact_us"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_light"
    tools:context="ir.adicom.caryar.ContactUsActivity"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:gravity="center">

    <com.cengalabs.flatui.views.FlatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="شرکت رایانه صنعت آذر پاد"
        android:textSize="16sp"/>

    <com.cengalabs.flatui.views.FlatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="site : www.rsatm.ir"
        android:textSize="16sp"
        android:id="@+id/flatTextView" />

    <com.cengalabs.flatui.views.FlatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="email : info@rsatm.ir"
        android:textSize="16sp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView6" />

    <com.cengalabs.flatui.views.FlatButton
        android:textSize="16sp"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        app:fl_touchEffect="fl_ripple"
        app:fl_blockButtonEffectHeight="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_margin="5dp"
        android:text="درباره اپلیکیشن نظر دهید" />
</LinearLayout>
